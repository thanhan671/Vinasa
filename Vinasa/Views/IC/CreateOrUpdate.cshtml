

@{
    ViewBag.Title = "CreateOrUpdate";
    Layout = "~/Views/Shared/sbadmin.cshtml";
}

<div class="heading">
    @*<h1 class="h3 mb-2 text-gray-800">Hội Nghị</h1>*@
    <nav aria-label="breadcrumb">
        <ol class="breadcrumb">
            <li class="breadcrumb-item" onclick="location.href = 'javascript:history.go(-1)'" style="color:blue; cursor:pointer" ;>Danh sách Các Quan Hệ Quốc Tế</li>
            <li class="breadcrumb-item active" aria-current="page">Thêm thông tin Sự Kiện Quốc Tế</li>
        </ol>
    </nav>
</div>
@*using (Html.BeginForm("Edit", "Member", FormMethod.Post))*@

<div class="row">
    <div class="col-6">
        <h5 class="heading">Thêm Thông Tin Sự Kiện Quốc Tế</h5>
        <form class="form-field" method="post" enctype="multipart/form-data">
            <div class="input-wrapper">
                <input class="ip-form" type="text" id="tax" contenteditable="true" required />
                <label class="lb-form" for="tax">Tên</label>
            </div>

            <div class="input-wrapper">
                <input class="ip-form" type="text" contenteditable="true" required />
                <label class="lb-form" for="name">Địa điểm</label>
            </div>

            <div class="input-wrapper">
                <input class="ip-form" type="text" contenteditable="true" required />
                <label class="lb-form" for="name">Thời Gian</label>
            </div>

        </form>
    </div>

</div>
<div class="button-form f-l">
    <button class="button-cancel" id="cancel">Huỷ</button>
    <button class="button-lc" type="submit" name="save">Xác Nhận</button>
</div>

<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
<script>

    $('#cancel').click(function () {
        location.href = 'javascript:history.go(-1)';
    })

</script>
@if (TempData["Message"] != null)
{
    <p class="alert alert-success" id="successMessage">@TempData["Message"]</p>
}