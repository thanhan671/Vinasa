@model Vinasa.Models.Seminar
@Html.ValidationSummary(true, "", new { @class = "text-danger" })
<div class="input-wrapper">
    @Html.EditorFor(model => model.Title, new { htmlAttributes = new { @class = "ip-form" } })
    @Html.LabelFor(model => model.Title, new { @class = "lb-form" })
    @Html.ValidationMessageFor(model => model.Title)
</div>
<div class="input-wrapper">
    @Html.EditorFor(model => model.OpenDate, new { htmlAttributes = new { @class = "ip-form", @type = "datetime-local" } })
    @Html.LabelFor(model => model.OpenDate, new { @class = "lb-form" })
    @Html.ValidationMessageFor(model => model.OpenDate)

</div>
<div class="input-wrapper">
    @Html.EditorFor(model => model.Address, new { htmlAttributes = new { @class = "ip-form"} })
    @Html.LabelFor(model => model.Address, new { @class = "lb-form" })
    @Html.ValidationMessageFor(model => model.Address)

</div>
<div class="button-form">
    <button class="button-lc">Xác Nhận</button>
    <button class="button-lc button-cancel" onclick="location.href = 'javascript:history.go(-1)'" id="cancel">Huỷ</button>
</div>
